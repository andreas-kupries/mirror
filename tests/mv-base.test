## Generic syntax errors for mirror-vcs

test mirror-vcs-1.0 "mirror-vcs - wrong args, not enough" -body {
    mvcs
} -result [err mirror-vcs/wrongargs-notenough] -match glob

test mirror-vcs-1.1 "mirror-vcs - wrong args, not enough" -body {
    mvcs VCS
} -result [err mirror-vcs/wrongargs-notenough] -match glob

test mirror-vcs-1.2 "mirror-vcs - wrong args, not enough" -body {
    mvcs VCS LOG
} -result [err mirror-vcs/wrongargs-notenough] -match glob
    
